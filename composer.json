{
    "name": "michaelze/cakephp-entityactions",
    "description": "EntityActions plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": ["cakephp", "entity", "action", "helper"],
    "homepage": "https://github.com/michaelze/cakephp-entityactions",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5"
    },
    "autoload": {
        "psr-4": {
            "EntityActions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EntityActions\\Test\\": "tests",
            "TestApp\\": "tests/test_app"
        }
    }
}
